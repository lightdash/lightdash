{
    "extends": "./../../tsconfig.json",
    "compilerOptions": {
        "composite": true,
        "target": "es5",
        "lib": [
            "es5",
            "dom"
        ],
        "types": [
            "cypress",
            "@testing-library/cypress",
            "node",
            "cypress-file-upload",
            "@playwright/test"
        ],
        "strictNullChecks": true, /* Enable strict null checks. */
        "esModuleInterop": true,
        "resolveJsonModule": true
    },
    "include": [
        "**/*.ts"
    ]
}