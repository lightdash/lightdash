{
    "extends": "./../../tsconfig.json",
    "compilerOptions": {
        "composite": true,
        "target": "es5",
        "lib": [
            "es5",
            "dom"
        ],
        "types": [
            "node",
            "@playwright/test"
        ],
        "strictNullChecks": true,
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "skipLibCheck": true
    },
    "include": [
        "playwright/**/*.ts"
    ]
}