{
    "name": "e2e",
    "version": "0.2530.1",
    "private": true,
    "license": "MIT",
    "main": "index.js",
    "scripts": {
        "cypress:open": "cypress open --e2e",
        "cypress:open:native": "RUNTIME=native cypress open --e2e",
        "cypress:run": "cypress run",
        "linter": "eslint -c .eslintrc.js --ignore-path ./../../.gitignore",
        "formatter": "oxfmt",
        "lint": "pnpm run linter ./cypress ./api-tests",
        "fix-lint": "pnpm run linter ./cypress ./api-tests --fix",
        "format": "oxfmt ./cypress ./api-tests --check",
        "fix-format": "oxfmt ./cypress ./api-tests",
        "test:api": "vitest run --config vitest.config.ts",
        "test:api:watch": "vitest watch --config vitest.config.ts"
    },
    "dependencies": {
        "@lightdash/common": "workspace:*",
        "@testing-library/cypress": "^10.0.3",
        "cypress": "^14.3.2",
        "cypress-file-upload": "^5.0.8",
        "cypress-split": "^1.24.19",
        "dayjs": "^1.11.10",
        "js-yaml": "^4.1.1"
    },
    "devDependencies": {
        "vitest": "^3.0.5"
    }
}
