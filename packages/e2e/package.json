{
    "name": "e2e",
    "version": "0.2520.2",
    "private": true,
    "license": "MIT",
    "main": "index.js",
    "scripts": {
        "cypress:open": "cypress open --e2e",
        "cypress:open:native": "RUNTIME=native cypress open --e2e",
        "cypress:run": "cypress run",
        "cypress:run:experimental": "CYPRESS_RETRIES=0 cypress run --config excludeSpecPattern=[] --spec 'cypress/e2e/experimental/**/*.cy.ts'",
        "cypress:open:experimental": "CYPRESS_RETRIES=0 cypress open --e2e --config 'specPattern=cypress/e2e/experimental/**/*.cy.ts,excludeSpecPattern=[]'",
        "linter": "eslint -c .eslintrc.js --ignore-path ./../../.gitignore",
        "formatter": "oxfmt",
        "lint": "pnpm run linter ./cypress",
        "fix-lint": "pnpm run linter ./cypress --fix",
        "format": "oxfmt ./cypress --check",
        "fix-format": "oxfmt ./cypress"
    },
    "dependencies": {
        "@lightdash/common": "workspace:*",
        "@testing-library/cypress": "^10.0.3",
        "cypress": "^14.3.2",
        "cypress-file-upload": "^5.0.8",
        "cypress-split": "^1.24.19",
        "dayjs": "^1.11.10",
        "js-yaml": "^4.1.1",
        "node-fetch": "^2.7.0"
    }
}
