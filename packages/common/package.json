        "dist/**/*"
    ],
    "license": "MIT",
    "devDependencies": {
        "@types/js-yaml": "^4.0.9",
        "@types/pegjs": "^0.10.3",
        "@types/sanitize-html": "^2.11.0",
        "@types/uuid": "^10.0.0"
    },
    "dependencies": {
        "@casl/ability": "^5.4.3",
        "@types/lodash": "^4.14.202",
        "ajv": "^8.3.0",
        "ajv-formats": "^2.1.0",
        "better-ajv-errors": "^1.2.0",
        "cronstrue": "^2.23.0",
        "dayjs": "^1.11.9",
        "dependency-graph": "^0.11.0",
        "js-yaml": "^4.1.0",
        "liquidjs": "^10.0.0",
        "lodash": "^4.17.21",
        "moment": "^2.29.4",
        "moment-timezone": "^0.5.45",
