{
    "name": "backend",
    "version": "0.4.0",
    "main": "dist/index",
    "license": "MIT",
    "devDependencies": {
        "@types/cookie-parser": "^1.4.2",
        "@types/express": "^4.17.11",
        "@types/express-session": "^1.17.4",
        "@types/jest": "^26.0.23",
        "@types/js-yaml": "^4.0.1",
        "@types/morgan": "^1.9.2",
        "@types/node": "^14.14.35",
        "@types/node-fetch": "^2.5.10",
        "@types/passport": "^1.0.7",
        "@types/passport-local": "^1.0.34",
        "eslint": "^7.29.0",
        "eslint-config-airbnb-base": "^14.2.1",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-import": "^2.23.4",
        "eslint-plugin-json": "^3.0.0",
        "jest": "^27.0.5",
        "jest-fetch-mock": "^3.0.3",
        "nodemon": "^2.0.7",
        "prettier": "2.3.2",
        "ts-jest": "^27.0.3",
        "ts-node": "^9.1.1",
        "typescript": "^4.2.3"
    },
    "dependencies": {
        "@stoplight/yaml": "^4.2.2",
        "@types/bcrypt": "^5.0.0",
        "@typescript-eslint/eslint-plugin": "^4.4.1",
        "ajv": "^8.3.0",
        "ajv-formats": "^2.1.0",
        "bcrypt": "^5.0.1",
        "body-parser": "^1.19.0",
        "common": "^0.4.0",
        "cookie-parser": "^1.4.5",
        "dependency-graph": "^0.11.0",
        "execa": "^5.0.0",
        "express": "^4.17.1",
        "express-session": "^1.17.2",
        "fuse.js": "^6.4.6",
        "js-yaml": "^4.1.0",
        "knex": "^0.95.7",
        "morgan": "^1.10.0",
        "node-cache": "^5.1.2",
        "node-fetch": "^2.6.1",
        "passport": "^0.4.1",
        "passport-local": "^1.0.0",
        "pg": "^8.6.0",
        "uuid": "^8.3.2"
    },
    "scripts": {
        "dev": "nodemon src/index.ts",
        "debug": "nodemon --exec \"node --inspect-brk=0.0.0.0:9229 --require ts-node/register src/index.ts\"",
        "build": "tsc --build tsconfig.json",
        "start": "node dist/index.js",
        "test": "jest",
        "lint": "eslint ./src",
        "format": "prettier --check --ignore-unknown ./src",
        "migrate": "knex migrate:up --knexfile src/database/knexfile.ts",
        "migrate-production": "knex migrate:up --knexfile dist/database/knexfile.js",
        "seed": "knex seed:run --knexfile src/database/knexfile.ts",
        "seed-production": "knex seed:run --knexfile dist/database/knexfile.js",
        "rollback-all": "knex migrate:rollback --all --knexfile src/database/knexfile.ts",
        "rollback-all-production": "knex migrate:rollback --all --knexfile dist/database/knexfile.js"
    }
}
