    "scripts": {
        "generate-api": "tsoa spec-and-routes --configuration tsoa.yml",
        "dev": "LIGHTDASH_MODE=development HEADLESS=true tsx watch --clear-screen=false src/index.ts",
        "debug": "tsx watch --clear-screen=false --inspect-brk=0.0.0.0:9229 src/index.ts ",
        "build": "tsc --build tsconfig.json",
        "postbuild": "copyfiles --error --up 1 src/**/*.html dist",
        "start": "node dist/index.js",
        "scheduler": "node dist/scheduler.js",
        "scheduler-dev": "tsx watch --clear-screen=false src/scheduler.ts",
        "test": "TZ=UTC jest",
        "linter": "eslint -c .eslintrc.js --ignore-path ./../../.gitignore  --ignore-pattern ./src/generated/*",
        "formatter": "prettier --config .prettierrc.js --ignore-unknown --ignore-path ./../../.gitignore",
        "lint": "yarn run linter ./src",
        "fix-lint": "yarn run linter ./src --fix",
        "format": "yarn run formatter ./src --check",
        "fix-format": "yarn run formatter ./src --write",
        "create-migration": "knex migrate:make --knexfile src/knexfile.ts",
        "migrate": "knex migrate:latest --knexfile src/knexfile.ts",
        "migrate-production": "knex migrate:latest --knexfile dist/knexfile.js",
        "seed": "knex seed:run --knexfile src/knexfile.ts",
        "seed-production": "knex seed:run --knexfile dist/knexfile.js",
        "rollback-last": "knex migrate:down --knexfile src/knexfile.ts",
        "rollback-all": "knex migrate:rollback --all --knexfile src/knexfile.ts",
        "rollback-all-production": "knex migrate:rollback --all --knexfile dist/knexfile.js"
    }
