{
    "extends": "./../../tsconfig.json",
    "compilerOptions": {
        "target": "ESNext",
        "declaration": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "module": "ESNext",
        "moduleResolution": "node",
        "strict": true,
        "outDir": "./dist",
        "jsx": "react-jsx",
        "jsxImportSource": "react",
        "lib": [
            "ESNEXT",
            "DOM",
            "DOM.Iterable"
        ]
    },
    "include": [
        "src"
    ],
}