{
    "name": "lightdash",
    "version": "0.1552.4",
    "main": "index.js",
    "license": "MIT",
    "private": true,
    "workspaces": [
        "packages/*"
    ],
    "resolutions": {
        "react-is": "19.0.0",
        "@types/react": "19.0.2",
        "@types/react-dom": "19.0.2",
        "typescript": "5.5.4"
    },
    "dependencies": {
        "tslib": "^2.8.1"
    },
    "devDependencies": {
        "@types/jest": "^29.5.5",
        "@types/node": "^22.13.1",
        "@types/pegjs": "^0.10.3",
        "@typescript-eslint/eslint-plugin": "^5.45.0",
        "@typescript-eslint/parser": "^5.45.0",
        "confusing-browser-globals": "^1.0.11",
        "danger": "^11.3.1",
        "dotenv": "^16.0.2",
        "dotenv-cli": "^6.0.0",
        "eslint": "^8.40.0",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-config-airbnb-typescript": "^17.0.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-jest": "^27.4.2",
        "eslint-plugin-json": "^3.1.0",
        "eslint-plugin-jsx-a11y": "^6.6.1",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-testing-library": "^5.6.3",
        "husky": "^9.0.0",
        "jest": "^29.7.0",
        "jest-watch-typeahead": "^2.2.2",
        "lint-staged": "^12.1.2",
        "prettier": "2.8.1",
        "prettier-plugin-organize-imports": "^3.1.1",
        "ts-jest": "^29.1.1",
        "tsx": "^4.19.2",
        "typescript": "5.5.4"
    },
    "scripts": {
        "preinstall": "node -e \"require('fs').existsSync('./scripts/only-allow-pnpm.js') && require('./scripts/only-allow-pnpm.js')\"",
        "common-dev": "pnpm -F @lightdash/common dev",
        "common-build": "pnpm -F @lightdash/common build",
        "common-lint": "pnpm -F @lightdash/common lint",
        "common-format": "pnpm -F @lightdash/common format",
        "common-fix-lint": "pnpm -F @lightdash/common fix-lint",
        "common-fix-format": "pnpm -F @lightdash/common fix-format",
        "common-test": "pnpm -F @lightdash/common test",
        "backend-dev": "pnpm -F backend dev",
        "backend-build": "pnpm -F backend build",
        "backend-start": "pnpm -F backend start",
        "backend-lint": "pnpm -F backend lint",
        "backend-format": "pnpm -F backend format",
        "backend-fix-lint": "pnpm -F backend fix-lint",
        "backend-fix-format": "pnpm -F backend fix-format",
        "backend-test": "pnpm -F backend test",
        "backend-test-sequential": "pnpm -F backend test-sequential",
        "frontend-dev": "pnpm -F frontend dev",
        "frontend-build": "pnpm -F frontend build",
        "frontend-build-lib": "pnpm -F frontend build-lib",
        "frontend-lint": "pnpm -F frontend lint",
        "frontend-format": "pnpm -F frontend format",
        "frontend-fix-lint": "pnpm -F frontend fix-lint",
        "frontend-fix-format": "pnpm -F frontend fix-format",
        "frontend-test": "pnpm -F frontend test",
        "sdk-build": "pnpm -F frontend build-lib && pnpm -F sdk build",
        "e2e-open": "pnpm -F e2e cypress:open",
        "e2e-run": "pnpm -F e2e cypress:run",
        "e2e-lint": "pnpm -F e2e lint",
        "e2e-format": "pnpm -F e2e format",
        "e2e-fix-lint": "pnpm -F e2e fix-lint",
        "e2e-fix-format": "pnpm -F e2e fix-format",
        "cli-test": "pnpm -F @lightdash/cli test",
        "cli-lint": "pnpm -F @lightdash/cli lint",
        "cli-format": "pnpm -F @lightdash/cli format",
        "cli-fix-lint": "pnpm -F @lightdash/cli fix-lint",
        "cli-fix-format": "pnpm -F @lightdash/cli fix-format",
        "cli-build": "pnpm -F @lightdash/cli build",
        "warehouses-dev": "pnpm -F @lightdash/warehouses dev",
        "warehouses-lint": "pnpm -F @lightdash/warehouses lint",
        "warehouses-format": "pnpm -F @lightdash/warehouses format",
        "warehouses-fix-lint": "pnpm -F @lightdash/warehouses fix-lint",
        "warehouses-fix-format": "pnpm -F @lightdash/warehouses fix-format",
        "warehouses-test": "pnpm -F @lightdash/warehouses test",
        "warehouses-build": "pnpm -F @lightdash/warehouses build",
        "lint": "pnpm -r --stream --sequential -F @lightdash/common -F @lightdash/warehouses -F backend -F frontend -F e2e -F @lightdash/cli lint",
        "format": "pnpm -r --stream --sequential -F @lightdash/common -F @lightdash/warehouses -F backend -F frontend -F e2e -F @lightdash/cli format",
        "fix-lint": "pnpm -r --stream --sequential -F @lightdash/common -F @lightdash/warehouses -F backend -F frontend -F e2e -F @lightdash/cli fix-lint",
        "fix-format": "pnpm -r --stream --sequential -F @lightdash/common -F @lightdash/warehouses -F backend -F frontend -F e2e -F @lightdash/cli fix-format",
        "test": "pnpm -r --stream --sequential -F @lightdash/common -F @lightdash/warehouses -F backend -F frontend -F e2e -F @lightdash/cli test",
        "dev": "pnpm -r --stream --parallel -F @lightdash/common -F @lightdash/warehouses -F backend -F frontend run dev",
        "build": "pnpm -r --stream --sequential -F @lightdash/common -F @lightdash/warehouses -F backend -F frontend build",
        "build-published-packages": "pnpm -r --stream --sequential -F @lightdash/common -F @lightdash/warehouses -F @lightdash/cli build",
        "release-packages": "pnpm -r --stream --sequential -F @lightdash/common -F @lightdash/warehouses -F @lightdash/cli release",
        "start": "pnpm run backend-start",
        "prepare": "husky || true",
        "load:env": "dotenv -c -e .env.development.local",
        "generate-api": "pnpm run common-build && pnpm -F backend generate-api && pnpm -F backend formatter --write ./src/generated",
        "generate-api-dev": "pnpm run common-build && pnpm -F backend generate-api-dev",
        "danger-local-lint": "danger -b main -f",
        "danger-ci-lint": "[[ -n $GITHUB_TOKEN ]] && danger ci --use-github-checks || echo 'Skipped - no GITHUB_TOKEN' present",
        "clean-node-modules": "./scripts/clean-node-modules.sh && pnpm install",
        "clean-build-cache": "./scripts/clean-build-cache.sh",
        "clean": "pnpm clean-build-cache && pnpm clean-node-modules",
        "clean-and-build": "pnpm clean && pnpm build"
    },
    "lint-staged": {
        "packages/frontend/src/**/*.(ts|tsx|json|css)": [
            "pnpm -F frontend run linter --fix",
            "pnpm -F frontend run formatter --write"
        ],
        "packages/backend/src/**/*.(ts|tsx|json)": [
            "pnpm -F backend run linter --fix",
            "pnpm -F backend run formatter --write"
        ],
        "packages/common/src/**/*.(ts|tsx|json)": [
            "pnpm -F @lightdash/common run linter --fix",
            "pnpm -F @lightdash/common run formatter --write"
        ],
        "packages/e2e/cypress/**/*.(ts|json)": [
            "pnpm -F e2e run linter --fix",
            "pnpm -F e2e run formatter --write"
        ],
        "packages/cli/src/**/*.(ts|tsx|json)": [
            "pnpm -F @lightdash/cli run linter --fix",
            "pnpm -F @lightdash/cli run formatter --write"
        ],
        "packages/warehouses/src/**/*.(ts|tsx|json)": [
            "pnpm -F @lightdash/warehouses run linter --fix",
            "pnpm -F @lightdash/warehouses run formatter --write"
        ]
    },
    "packageManager": "pnpm@9.15.5+sha512.845196026aab1cc3f098a0474b64dfbab2afe7a1b4e91dd86895d8e4aa32a7a6d03049e2d0ad770bbe4de023a7122fb68c1a1d6e0d033c7076085f9d5d4800d4"
}
